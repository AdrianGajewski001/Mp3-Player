<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Mp3_Player"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
    
    <FontFamily x:Key="FontAwesome">pack://appliction;,,,/Fonts/#FontAwesome</FontFamily>

    <system:String x:Key="FontAwesomeMinimizeIcon">&#xf2d1;</system:String>
    <system:String x:Key="FontAwesomeCloseIcon">&#xf00d;</system:String>
    <system:String x:Key="FontAwesomePlayIcon">&#xf144;</system:String>
    <system:String x:Key="FontAwesomePauseIcon">&#xf28b;</system:String>
    <system:String x:Key="FontAwesomeLeftArrowIcon">&#xf100;</system:String>
    <system:String x:Key="FontAwesomeRightArrowIcon">&#xf101;</system:String>
    <system:String x:Key="FontAwesomeFileIcon">&#xf1c7;</system:String>
    
</ResourceDictionary>